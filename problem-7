#include<stdio.h>
#include<stdlib.h>
int main()
{
 int j=1,c=1,i=2,count=0,cnt=0,n=2000000,sum=0;
 for(count=2;count<=n;)
 {
   for(c=2;c<=i-1;c++)
   {
     if(i%c==0)
	   break;
  
   }
   if(c==i)
   {
    cnt++;
    if(cnt==10001)
    {
     printf("%d\n",i);
     exit(0);   
    }
  } 
   count++;
   i++;
 }
  return 0;
}
